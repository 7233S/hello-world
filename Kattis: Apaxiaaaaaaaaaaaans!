#include <iostream>
using namespace std;

int main(){
    string s;
    cin >> s;
    
    unsigned i; //I originally used int but there's a weird error message on comparing different signs or something
    
    cout << s[0];
    
    for (i=1; i<s.size(); i++) {
        if (s[i] != s[i-1]) {
            cout << s[i];
        }
    }
    
    return 0;
}
