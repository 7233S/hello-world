#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n;
    cin >> n;
    
    int ori[n];
    int final[n];
    int sum = 0;
    
    for (int i=0; i<n; i++) {
        cin >> ori[i];
        final[i] = pow((ori[i]/10),ori[i]%10);
        sum += final[i];
    }
    
    cout << sum;
    
    return 0;
}

//Difficulty 1.3
